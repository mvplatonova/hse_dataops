# =============================================================================
# JupyterHub Configuration
# =============================================================================

# Generate secret keys with: openssl rand -hex 32
JUPYTERHUB_CRYPT_KEY=your_64_char_hex_string_here
JUPYTERHUB_COOKIE_SECRET=your_64_char_hex_string_here
CONFIGPROXY_AUTH_TOKEN=your_proxy_token_here

# JupyterHub port
JUPYTERHUB_PORT=8000

# Admin users (comma-separated)
JUPYTERHUB_ADMIN=admin

# =============================================================================
# PostgreSQL Database
# =============================================================================

POSTGRES_HOST=db
POSTGRES_PORT=5432
POSTGRES_DB=jupyterhub_db
POSTGRES_USER=jupyterhub_user
POSTGRES_PASSWORD=your_secure_password_here

# =============================================================================
# Additional Settings
# =============================================================================

TZ=UTC
PYTHONUNBUFFERED=1
